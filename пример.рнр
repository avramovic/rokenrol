интерфејс ПримерИнтерфејс {
    јавна функција пример();
}

класа Пример задовољава ПримерИнтерфејс {
    јавно €име = 1;
    јавна функција пример() {
        врати 'пример';
    }
}

класа Пример2 наслеђује Пример {
    јавно €име = 2;

    јавна функција пример() {
        врати 'пример2';
    }
}


//

€низ = [нов Пример, нов Пример2];

засваки (€низ као €пример) {
    акоје (€пример->пример() == 'пример') {
        пиши €пример->име, ' долази са ', €пример->пример(), НОВИРЕД;
    } иначе {
        пиши €пример->име, '? врати се...', €пример->пример(), НОВИРЕД;
    }

}
